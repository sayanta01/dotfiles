{
  // "editor.fontFamily": "JetBrainsMono NF",
  // "editor.fontLigatures": true,
  "editor.fontSize": 19,
  "terminal.integrated.fontSize": 19,
  "extensions.ignoreRecommendations": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "off",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.matchBrackets": "near",
  "editor.renderWhitespace": "none",
  "editor.cursorBlinking": "solid",
  "editor.bracketPairColorization.enabled": false,
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.iconTheme": "material-icon-theme",
  // "workbench.editor.highlightModifiedTabs": true,
  // "workbench.editor.enablePreview": false,
  "vim.highlightedyank.enable": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  // "vim.insertModeKeyBindings": [
  //   //{ "before": ["j", "k"], "after": ["<Esc>"] }
  // ],
  "vim.normalModeKeyBindings": [
    { "before": ["<S-l>"], "commands": ["workbench.action.nextEditor"] },
    { "before": ["<S-h>"], "commands": ["workbench.action.previousEditor"] },
    { "before": ["leader", "q"], "commands": [":q!"] },
    { "before": ["leader", "x"], "commands": [":bdelete"] },
    { "before": ["<Esc>"], "commands": [":noh"] },
    { "before": ["g", "D"], "commands": ["editor.action.revealDeclaration"] },
    { "before": ["g", "d"], "commands": ["editor.action.revealDefinition"] },
    { "before": ["<S-k>"], "commands": ["editor.action.showHover"] },
    { "before": ["g", "i"], "commands": ["editor.action.goToImplementation"] },
    {
      "before": ["<leader>", "D"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    { "before": ["<leader>", "r", "n"], "commands": ["editor.action.rename"] },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.codeAction"]
    },
    { "before": ["g", "r"], "commands": ["editor.action.goToReferences"] },
    { "before": ["[", "d"], "commands": ["editor.action.marker.prev"] },
    { "before": ["]", "d"], "commands": ["editor.action.marker.next"] },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    { "before": ["<leader>", "l", "q"], "commands": ["editor.action.quickFix"] }
  ],
  "vim.visualModeKeyBindings": [
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    { "before": ["<"], "commands": ["editor.action.outdentLines"] }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // { "before": ["<leader>", "e"], "commands": ["workbench.view.explorer"] }
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "telemetry.telemetryLevel": "off"
}
