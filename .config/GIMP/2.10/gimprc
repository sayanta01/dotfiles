# GIMP gimprc

(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(monitor-xresolution 102)
(monitor-yresolution 102)
(can-change-accels yes)
(toolbox-groups no)
(theme "System")
(icon-theme "Color")
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))

# end of gimprc
