{
  "layer": "top",
  "position": "top",
  "exclusive": true,
  "passthrough": false,
  "ipc": false,
  "tooltip": false,
  // "gtk-layer-shell": true,
  "modules-left": [
    "hyprland/workspaces"
  ],
  "modules-right": [
    "custom/date",
    "custom/nettraf",
    "pulseaudio",
    "custom/memory",
    // "battery",
    "tray"
  ],
  "hyprland/workspaces": {
    "all-outputs": true,
    "on-click": "activate",
    "format": "{icon}",
    "format-icons": {
      "1": "ó°Œ€",
      "2": "îª…",
      "3": "ó°¯ˆ",
      "4": "ó±‡",
      "5": "î˜«",
      "6": "ï‡™",
      "7": "ó±…",
      "8": "ï‘‡",
      "9": "î°›",
      "10": "ï’º"
    }
  },
  "custom/separator": {
    "tooltip": false,
    "format": "/",
    "interval": "once"
  },
  "tray": {
    "icon-size": 18,
    "spacing": 6
  },
  "custom/nettraf": {
    "tooltip": false,
    "interval": 4,
    "exec": "~/.local/bin/statusbar/sb-nettraf",
    "on-click-right": "kitty --class floating -e nmtui"
    // "return-type": "json"
  },
  "custom/memory": {
    "tooltip": false,
    "interval": 4,
    "exec": "~/.local/bin/statusbar/sb-mem",
    // "on-click": "notify-send \"$(ps axch -o cmd:16,%mem --sort=-%mem | head)\"",
    "on-click-right": "kitty --class floating -e htop"
  },
  "custom/date": {
    "tooltip": false,
    "interval": 2,
    "exec": "~/.local/bin/statusbar/sb-time"
  },
  "pulseaudio": {
    "tooltip": false,
    "format": "{icon} {volume}%",
    "format-muted": "ğŸ”‡ Muted",
    "on-click": "pamixer -t",
    "on-click-right": "pavucontrol",
    "on-scroll-up": "pamixer -i 5",
    "on-scroll-down": "pamixer -d 5",
    "scroll-step": 5,
    "format-icons": {
