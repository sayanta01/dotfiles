"packadd! dracula
"colorscheme dracula

set number
set nocompatible
set nobackup                  
set history=100
syntax enable
syntax on
set encoding=utf-8
set hlsearch
set ignorecase
set smartcase
set smartindent
set showmatch
set incsearch
set nohlsearch
set laststatus=2
set background=dark
set t_Co=256
set scrolloff=7
set nowrap

"set relativenumber
"set spell
"set hidden



